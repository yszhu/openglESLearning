<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.admin.openglesnote">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".notes.HelloTriangle"/>
        <activity android:name=".notes.TriangleWithLoader"/>
        <activity android:name=".notes.VertexBufferObject"/>
        <activity android:name=".notes.VertexArrayObject"/>
        <activity android:name=".notes.MapBufferObject"/>

        <activity android:name=".NoteListActivity"/>

        <activity android:name=".CheckingActivity"/>
        <activity android:name=".DrawingActivity"/>
    </application>

</manifest>